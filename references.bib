},

@article{court2005,
	title = {How the human telomeric proteins {TRF1} and {TRF2} recognize telomeric {DNA:} a view from high-resolution crystal structures},
	volume = {6},
	issn = {{1469-221X}},
	shorttitle = {How the human telomeric proteins {TRF1} and {TRF2} recognize telomeric {DNA}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/15608617},
	doi = {10.1038/sj.embor.7400314},
	abstract = {Human telomeres consist of tandem arrays of {TTAGGG} sequence repeats that are specifically bound by two proteins, {TRF1} and {TRF2.} They bind to {DNA} as preformed homodimers and have the same architecture in which the {DNA-binding} domains {(Dbds)} form independent structural units. Despite these similarities, {TRF1} and {TRF2} have different functions at telomeres. The X-ray crystal structures of both {TRF1-} and {TRF2-Dbds} in complex with telomeric {DNA} (2.0 and 1.8 angstroms resolution, respectively) show that they recognize the same {TAGGGTT} binding site by means of homeodomains, as does the yeast telomeric protein Rap1p. Two of the three {G-C} base pairs that characterize telomeric repeats are recognized specifically and an unusually large number of water molecules mediate {protein-DNA} interactions. The binding of the {TRF2-Dbd} to the {DNA} double helix shows no distortions that would account for the promotion of t-loops in which {TRF2} has been implicated.},
	number = {1},
	journal = {{EMBO} Reports},
	author = {Court, Robert and Chapman, Lynda and Fairall, Louise and Rhodes, Daniela},
	month = jan,
	year = {2005},
	note = {{PMID:} 15608617},
	keywords = {citation-these, Crystallography, Human, Myb, {PDB}, Protein - {DNA}, Telomeres, {TRF1}, {TRF2}},
	pages = {39--45}
},

@article{cui2007,
	title = {Escherichia coli with a linear genome},
	volume = {8},
	copyright = {Copyright {\textcopyright} 2007 European Molecular Biology Organization},
	issn = {{1469-221X}, 1469-3178},
	url = {http://embor.embopress.org/content/8/2/181},
	doi = {10.1038/sj.embor.7400880},
	abstract = {Chromosomes in eukaryotes are linear, whereas those of most, but not all, prokaryotes are circular. To explore the effects of possessing a linear genome on prokaryotic cells, we linearized the Escherichia coli genome using the lysogenic $\lambda$-like phage N15. Linear genome E. coli were viable and their genome structure was stable. There were no appreciable differences between cells with linear or circular genomes in growth rates, cell and nucleoid morphologies, genome-wide gene expression (with a few exceptions), and {DNA} gyrase- and topoisomerase {IV-dependent} growth. However, under dif-defective conditions, only cells with a circular genome developed an abnormal phenotype. Microscopy indicated that the ends of the linear genome, but not the circular genome, were separated and located at each end of a new-born cell. When tos{\textemdash}the cis-element required for linearization{\textemdash}was inserted into different chromosomal sites, those strains with the genome termini that were more remote from dif showed greater growth deficiencies.},
	number = {2},
	journal = {{EMBO} reports},
	author = {Cui, Tailin and Moro-oka, Naoki and Ohsumi, Katsufumi and Kodama, Kenichi and Ohshima, Taku and Ogasawara, Naotake and Mori, Hirotada and Wanner, Barry and Niki, Hironori and Horiuchi, Takashi},
	month = feb,
	year = {2007},
