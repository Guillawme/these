	author = {Adams, Paul D and Afonine, Pavel V and Bunk\'{o}czi, G\'{a}bor and Chen, Vincent B and Davis, Ian W and Echols, Nathaniel and Headd, Jeffrey J and Hung, {Li-Wei} and Kapral, Gary J and {Grosse-Kunstleve}, Ralf W and {McCoy}, Airlie J and Moriarty, Nigel W and Oeffner, Robert and Read, Randy J and Richardson, David C and Richardson, Jane S and Terwilliger, Thomas C and Zwart, Peter H},
