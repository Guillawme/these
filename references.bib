@article{garavis2013,
	title = {On the Origin of the Eukaryotic Chromosome: The Role of Noncanonical {DNA} Structures in Telomere Evolution},
	volume = {5},
	issn = {, 1759-6653},
	shorttitle = {On the Origin of the Eukaryotic Chromosome},
	url = {http://gbe.oxfordjournals.org/content/5/6/1142},
	doi = {10.1093/gbe/evt079},
	abstract = {The transition of an ancestral circular genome to multiple linear chromosomes was crucial for eukaryogenesis because it allowed rapid adaptive evolution through aneuploidy. Here, we propose that the ends of nascent linear chromosomes should have had a dual function in chromosome end protection (capping) and chromosome segregation to give rise to the {\textquotedblleft}proto-telomeres.{\textquotedblright} Later on, proper centromeres evolved at subtelomeric regions. We also propose that both noncanonical structures based on guanine{\textendash}guanine interactions and the end-protection proteins recruited by the emergent telomeric heterochromatin have been required for telomere maintenance through evolution. We further suggest that the origin of Drosophila telomeres may be reminiscent of how the first telomeres arose.},
	number = {6},
	journal = {Genome Biology and Evolution},
	author = {Garav\'{i}s, Miguel and Gonz\'{a}lez, Carlos and Villasante, Alfredo},
	month = jan,
	year = {2013},
